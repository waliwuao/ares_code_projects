<?xml version="1.0"?>
<sdf version="1.7">
  <world name="basketball_world">
    <!-- 环境设置 -->
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- 加载篮球模型 -->
    <model name="basketball_1">
      <include>
        <uri>model://basketball</uri>  <!-- 确保模型路径正确 -->
      </include>
      <pose>0 0 0.3 0 0 0</pose>       <!-- 初始位置 (x y z roll pitch yaw) -->
    </model>

    <!-- 其他模型或配置 -->
  </world>
</sdf>

